import java.util.HashSet;
import java.util.Set;
import java.util.TreeSet;

public class App {
	public static void main(String[] args) {
		// HashSet nie ma ¿adnego posortowania
		// Set<String> set1 = new HashSet<String>();
		// LinkedHashSet zapamiêtuje kolejnoœæ dodania
		// Set<String> set1 = new LinkedHashSet<String>();
		// TreeSet sortuje alfabetycznie
		Set<String> set1 = new TreeSet<String>();
		if (set1.isEmpty()) {
			System.out.println("Pusty na starcie!");
		}
		set1.add("dog");
		set1.add("cat");
		set1.add("mouse");
		set1.add("snake");
		set1.add("spider");

		if (set1.isEmpty()) {
			System.out.println("Pusty po dodawaniu!");
		}
		// Dodawanie duplikatów nie robi nic
		set1.add("mouse");

		System.out.println(set1);

		// /////////////////ITERATION/////////////
		// wypisanie w kolejnoœci zale¿nej od u¿ytego set'u
		for (String element : set1) {
			System.out.println(element);
		}
		// ///// Sprawdzanie czy zbiór zawiera
		if (set1.contains("snake")) {
			System.out.println("Zawiera snake");
		} else {
			System.out.println("Nie zawiara");
		}
		// set2 zawiera kilka takich samych elementów jak set1 i kilka ró¿nych
		Set<String> set2 = new TreeSet<String>();

		set2.add("dog");
		set2.add("cat");
		set2.add("giraffe");
		set2.add("monkey");
		set2.add("ant");
		// ///////// Intersection ///////////
		Set<String> intersection = new HashSet<String>(set1);
		// System.out.println(intersection);
		intersection.retainAll(set2);
		System.out.println(intersection);
		
		///////// Ró¿nice ///////////
		Set<String> difference = new HashSet<String>();
		difference.removeAll(set2);
		System.out.println(difference);
	}
}
