public class App {
	public static void main(String[] args) {
		int inValue = 888;
		short shortValue = 55;
		byte byteValue = 20;
		long longValue = 233333;
		float floatValue = 3452.46f;
		double doubleValue = 23.643;
		
		System.out.println(Double.MAX_VALUE);
		System.out.println(Byte.MAX_VALUE);
		
	}
}
